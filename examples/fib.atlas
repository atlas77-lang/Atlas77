import "std/io"

func main()  {
    let fib_10 = recursive_fib(10);
    println(fib_10 as str);
    fib_10;
}

func recursive_fib(n: int64) -> int64 {
    if n <= 1 {
        return n;
    } else {
        return recursive_fib(n - 1) + recursive_fib(n - 2);
    }
}

func iterative_fib(n: int64) -> int64 {
    let a = 0;
    let b = 1;
    let i = 0;
    while i < n {
        let temp = a;
        a = b;
        b = temp + b;
        i = i + 1;
    }
    return a;
}
