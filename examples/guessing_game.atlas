import "std/math"


func main() {
    let secret: int64 = random(1, 100);
    let guess = 0;
    while guess != secret {
        print("Guess the number: ");
        guess = input();
        if guess < secret {
            println("Too low!");
        } else if guess > secret {
            println("Too high!");
        } else {
            println("You win!");
        }
    }
}