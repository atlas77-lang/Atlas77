import "std/io"

public class Test {
    public:
        data: int64;
    public:
        Test(data: int64) {
            self.data = data;
        }
        func uh(self) {
            println(self.data);
            return;
        }
        func uh2(self, data: int64) {
            println(data);
            return;
        }
}

public concept Builder<T, U> {
    T(arg: U) -> T;
}

class Builder: impl Builder<T, U> {
    public:
        func build<T, U>(t: T, arg: U) -> T {
            return new T(arg);
        }
}

func main() {
    let test = new Test(42);
    println(test.data);
    test.uh();
    test.uh2(42);
    //test.uh3(42);
}