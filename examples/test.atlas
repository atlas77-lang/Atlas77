//Should return a warning because "string" is not PascalCase
//classes are ignored during compilation for now
public class String<T: Display + Foo, U: Display> {
    public:
        const HELLO_WORLD: [char] = ['H', 'e', 'l', 'l', 'o', ' ', 'W', 'o', 'r', 'l', 'd', '!'];
    private:
        s: [char];
        len: uint64;
    public:
        String(s: [char]) {
            self.len = len(s);
            len.s = s;
        }
        ~String() {
            delete self;
        }
        func hello_world() -> String {
            return new String(String::HELLO_WORLD);
        }
        func len(self) -> uint64 {
            return self.len;
        }
        operator +(self, other: String) -> String {
            let new_s: [char] = new [char; self.len + other.len];
            let i = 0;
            while i < self.len {
                new_s[i] = self.s[i];
                i = i + 1;
            }
            let j = 0;
            while j < other.len {
                new_s[i] = other.s[j];
                i = i + 1;
                j = j + 1;
            }
            return new String(new_s);
        }
}
