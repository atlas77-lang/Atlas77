public class Int64 {
    public:
        value: int64;
    public:
        Int64(value: int64) {
            self.value = value;
        }
        ~Int64() {
            //delete self.value;
        }
        func to_str(self) -> str {
            return self.value as str;
        }
        func to_int(self) -> int64 {
            return self.value;
        }
        func zeroed() -> Int64 {
            return new Int64(0);
        }
}